{
  "name": "slidev-theme-SUPSISTUDENT",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme-supsistudent",
    "slidev-theme",
    "slidev",
    "supsistudent"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^51.4.0"
  },
  "devDependencies": {
    "@slidev/cli": "^51.4.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "transition": "slide-left",
      "aspectRatio": "16:9",
      "colorSchema": "light",
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}
